{
  "manifest_version": 2,
  "name": "ModDash",
  "description": "Quickly switch to other open tabs with just your keyboard",
  "version": "1.0.0",
  "background": {
    "persistent": true,
    "page": "./src/chrome/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "./public/dist/index.html"
  },
  "permissions": [
    "cookies",
    "notifications",
    "background",
    "identity",
    "storage",
    "geolocation"
  ],
  "oauth2": {
    "client_id": "620726441303-ba0bp99k03erdsr829sclks4cttst15u.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login", "https://www.googleapis.com/auth/plus.profile.emails.read"]
  },
  "key": "",
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.14.1/ https://cdn.pubnub.com/ http://localhost:9000; object-src 'self' "
}
